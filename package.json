{
  "name": "velitherm-visu",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "bootstrap": "^5.3.8",
    "react-dom": "^19.1.1",
    "react-intl": "^8.0.11",
    "react-repeatable": "^2.0.1",
    "react-router-dom": "^7.12.0",
    "velitherm": "^2.0.1",
    "vite-plugin-svgr": "^4.5.0"
  },
  "scripts": {
    "start": "VITE_BUILD=\"dev-`git log --format='%h' -n 1` from `date -I`\" npx vite",
    "build": "VITE_BUILD=\"`git log --format='%h' -n 1` from `date -I`\" npx vite build",
    "serve": "npx vite preview",
    "dev": "npx vite dev",
    "extract": "npx formatjs extract src/App.tsx src/comp/*.tsx --out-file lang/en.json --format simple",
    "compile": "npx formatjs compile --format simple lang/fr.json --ast --out-file src/compiled-lang/fr.json && formatjs compile --format simple lang/en.json --ast --out-file src/compiled-lang/en.json",
    "deploy": "tar -C build -zcf - . | ssh aircalc@orel.garga.net 'tar -C www -zxvf -'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@formatjs/cli": "^6.10.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite-plugin-eslint": "^1.8.1"
  },
  "license": "GPL-3.0"
}
